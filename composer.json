{
    "name": "dos/oauth-server-bundle",
    "type": "symfony-bundle",
    "description": "Symfony OAuthServerBundle",
    "keywords": ["oauth", "oauth", "security"],
    "homepage": "http://github.com/liverbool/dos-oauth-server-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Ishmael Doss",
            "email": "nukboon@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "doctrine/orm": "^2.4",
        "league/oauth2-server": "^6.1",
        "phpmob/media-bundle": "^1.0",
        "phpseclib/phpseclib": "^2.0",
        "sylius/resource-bundle": "^1.2",
        "sylius/user": "^1.2",
        "symfony/console": "^3.4 || ^4.1",
        "symfony/psr-http-message-bridge": "^1.1",
        "zendframework/zend-diactoros": "^2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.8",
